<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    
	<Button android:text="Regenerate Static" android:layout_alignParentTop="true"
		android:id="@+id/regenButton" android:layout_width="fill_parent"
		android:layout_height="wrap_content"></Button>


	<ImageView android:id="@+id/ImageView01" android:scaleType="fitStart"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:layout_below="@+id/regenButton" />
    
<com.google.ads.AdView android:id="@+id/adView"
		android:layout_alignParentBottom="true" android:layout_width="fill_parent"
		android:layout_height="wrap_content" ads:adUnitId="a14defa3da93abf"
		ads:adSize="BANNER" ads:loadAdOnCreate="false" />

</RelativeLayout>
